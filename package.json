    "eject": "react-scripts eject",
    "prettier": "prettier --write 'src/**/*.{js,ts,tsx,md,graphql}'",
    "generate-fragments": "node -r dotenv/config -r ts-node/register ./src/schemaQuery.ts",
    "apollo": "apollo service:download --endpoint=http://localhost:4000/ graphql-schema.json && rm -rf src/generated && apollo codegen:generate --localSchemaFile=graphql-schema.json --excludes=node_modules/* --target typescript --tagName gql --outputFlat src/generated"
